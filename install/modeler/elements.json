			},
			{
				"label": "Process Variables",
				"type": "Text",
				"binding": {
					"type": "camunda:field",
					"name": "procVariablesExpr",
					"expression": true
				},
				"constraints": {
					"pattern": {
						"value": "(^([^,]+=[^,]+|[^,]+=[^,]+(,[^,]+=[^,]+)+)$)|(^\\$\\{[a-zA-Z_].*\\}$)",
						"message": "Must provide comma separated mapping variables.  For example: food=pizza,level=1" 
					}
				}
				
			},
			{
				"label": "Process Business Key",
				"type": "String",
				"binding": {
					"type": "camunda:field",
					"name": "procBusinessKeyExpr",
					"expression": true
				},
				"constraints": {
					"notEmpty": true
				}
			},
			{
				"label": "Initiation Key",
				"type": "String",
				"value": "process instance: ${execution.processInstanceId}",
				"binding": {
					"type": "camunda:field",
					"name": "initiationKeyExpr",
					"expression": true
				},
				"constraints": {
					"notEmpty": true
				}
			},
			{
				"label": "Priority",
				"type": "String",
				"value": "10",
				"binding": {
					"type": "camunda:field",
					"name": "priorityExpr",
					"expression": true
				},
				"constraints": {
					"notEmpty": true,
					"mandatory": true,
					"min": 0,
					"max": 2147483647,
					"pattern": {
						"value": "(^[0-9]+\\d*$)|(^\\$\\{[a-zA-Z_].*\\}$)",
						"message": "Must be a positive integer."
					}
				}
			},
			{
				"label": "Pre-condition",
				"type": "String",
				"value": "none",
				"binding": {
					"type": "camunda:field",
					"name": "preCondition",
					"expression": true
				},
				"constraints": {
					"notEmpty": false
				}
			},
			{
				"label": "On Pre-condition Fail",
				"type": "Dropdown",
				"value": "ABORT_PROCESS",
				"choices": [
					{ "name": "ABORT_PROCESS", "value": "ABORT_PROCESS" },
					{ "name": "SKIP_TASK", "value": "SKIP_TASK" }
				],
				"binding": {
					"type": "camunda:field",
					"name": "onPreConditionFail",
					"expression": true
				}
			},
			{
				"label": "Implementation",
				"type": "Hidden",
				"value": "null",
				"editable": false,
				"binding": {
					"type": "property",
					"name": "camunda:type"
				}
			}
		]
