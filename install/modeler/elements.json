[
	{
		"$schema": "https://unpkg.com/@camunda/element-templates-json-schema/resources/schema.json",
		"name": "Command Line Execution",
		"id": "jpl.cws.task.CmdLineExecTask",
		"appliesTo": [
			"bpmn:ServiceTask"
		],
		"properties": [
			{
				"label": "Command",
				"type": "Text",
				"binding": {
					"type": "camunda:field",
					"name": "cmdLine",
					"expression": true
				},
				"constraints": {
					"notEmpty": true
				}
			},
			{
				"label": "Working Directory",
				"type": "String",
				"binding": {
					"type": "camunda:field",
					"name": "workingDir",
					"expression": true
				},
				"constraints": {
					"notEmpty": true
				}
			},
			{
				"label": "Success Value(s)",
				"type": "String",
				"binding": {
					"type": "camunda:field",
					"name": "successExitValues",
					"expression": true
				},
				"constraints": {
					"notEmpty": true
				}
			},
			{
				"label": "Throw BpmnError on Failure?",
				"type": "Dropdown",
				"value": "true",
				"choices": [
					{ "name": "True", "value": "true" },
					{ "name": "False", "value": "false" }
				],
				"binding": {
					"type": "camunda:field",
					"name": "throwOnFailures",
					"expression": true
				}
			},
			{
				"label": "Exit Event Map",
				"type": "Text",
				"binding": {
					"type": "camunda:field",
					"name": "exitCodeEvents",
					"expression": true
				},
				"constraints": {
					"notEmpty": true,
					"pattern": {
						"value": "(^([^,]+=[^,]+|[^,]+=[^,]+(,[^,]+=[^,]+)+)$)|(^\\$\\{[a-zA-Z_].*\\}$)",
						"message": "Must provide comma separated mapping of exit values to event variables.  For example: 0=success,1=error"
					}
				}
			},
			{
				"label": "Throw BpmnError on Truncated Variable?",
				"type": "Dropdown",
				"value": "false",
				"choices": [
					{ "name": "True", "value": "true" },
					{ "name": "False", "value": "false" }
				],
				"binding": {
					"type": "camunda:field",
					"name": "throwOnTruncatedVariable",
					"expression": true
				}
			},
			{
				"label": "Pre-condition",
				"type": "String",
				"binding": {
					"type": "camunda:field",
					"name": "preCondition",
					"expression": true
				}
			},
			{
				"label": "On Pre-condition Fail",
				"type": "Dropdown",
				"value": "ABORT_PROCESS",
				"choices": [
					{ "name": "ABORT_PROCESS", "value": "ABORT_PROCESS" },
					{ "name": "SKIP_TASK", "value": "SKIP_TASK" }
				],
				"binding": {
					"type": "camunda:field",
					"name": "onPreConditionFail",
					"expression": true
				}
			},
			{
				"label": "Timeout (in seconds)",
				"type": "String",
				"value": "86400",
				"binding": {
					"type": "camunda:field",
					"name": "timeout",
					"expression": true
				},
				"constraints": {
					"notEmpty": true,
					"pattern": {
					  "value": "(^[1-9][0-9]*$)|(^\\$\\{[a-zA-Z_].*\\}$)",
					  "message": "Must provide a timeout duration, in seconds, allowed for the command to run before retrying." 
					}
				}
			},
			{
				"label": "Retries",
				"type": "String",
				"value": "0",
				"binding": {
					"type": "camunda:field",
					"name": "retries",
					"expression": true
				},
				"constraints": {
					"notEmpty": true,
					"pattern": {
					  "value": "(^[0-9][0-9]*$)|(^\\$\\{[a-zA-Z_].*\\}$)",
					  "message": "Must provide the maximum number of retries to run the command, if it fails or times out." 
					}
				}
			},
			{
				"label": "Retry Delay (in milliseconds)",
				"type": "String",
				"value": "1000",
				"binding": {
					"type": "camunda:field",
					"name": "retryDelay",
					"expression": true
				},
				"constraints": {
					"notEmpty": true,
					"pattern": {
					  "value": "(^[0-9][0-9]*$)|(^\\$\\{[a-zA-Z_].*\\}$)",
					  "message": "Must provide the duration, in milliseconds, to wait before retrying." 
					}
				}
			},
			{
				"label": "Implementation",
				"type": "Hidden",
				"value": "external",
				"editable": false,
				"binding": {
					"type": "property",
					"name": "camunda:type"
				}
			},
			{
				"label": "Topic",
				"type": "Hidden",
				"value": "__CWS_CMD_TOPIC__",
				"editable": false,
				"binding": {
					"type": "property",
					"name": "camunda:topic"
				}
			}
		]
	},
