		"appliesTo": [
			"bpmn:ServiceTask"
		],
		"properties": [
			{
				"label": "Implementation Type",
				"type": "Hidden",
				"value": "jpl.cws.task.ScheduleProcTask",
				"editable": false,
				"binding": {
